{
  "name": "form-prefill",
  "version": "1.2.0",
  "description": "Prefill HTML forms from flat GET params and optional base64url(JSON) payload with simple {{vars}}.",
  "main": "dist/form-prefill.js",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "build:copy": "cp src/form-prefill.js dist/form-prefill.js",
    "build:min": "esbuild src/form-prefill.js --format=iife --minify --legal-comments=none --outfile=dist/form-prefill.min.js",
    "build": "npm run clean && npm run build:copy && npm run build:min",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "esbuild": "^0.27.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafalmasiarek/js-form-prefill.git"
  },
  "keywords": [
    "form",
    "prefill",
    "querystring",
    "payload",
    "mustache"
  ],
  "author": "Rafal Masiarek <rafal@masiarek.pl>",
  "bugs": {
    "url": "https://github.com/rafalmasiarek/js-form-prefill/issues"
  },
  "homepage": "https://github.com/rafalmasiarek/js-form-prefill#readme"
}
